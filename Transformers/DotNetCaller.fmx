# =========================================================================
TRANSFORMER_NAME: DotNetCaller
# =========================================================================

VERSION: 0

CATEGORY: Infrastructure

INPUT_TAGS: <BLANK>

OUTPUT_TAGS: OUTPUT

PRESERVES_ATTRIBUTES: Yes

FEATURE_HOLDING: NONE

ATTRIBUTES_ADDED:

PARAMETER_NAME: TRANSFORMER_GROUP
PARAMETER_TYPE: GROUP XFORMER_NAME
PARAMETER_PROMPT: Transformer
PARAMETER_DEFAULT:

PARAMETER_NAME: PARAMETERS_GROUP
PARAMETER_TYPE: GROUP DOTNET_ASSEMBLY_FILENAME_ATTR%DOTNET_ASSEMBLY_FACTORY_NAME_ATTR%DOTNET_ASSEMBLY_FACTORY_PARAMS_ATTR
PARAMETER_PROMPT: Parameters
PARAMETER_DEFAULT:

PARAMETER_NAME: DOTNET_ASSEMBLY_FILENAME_ATTR
PARAMETER_TYPE: FILENAME Assembly_Files(*.dll)|*.dll
PARAMETER_PROMPT: .NET assembly filename:
PARAMETER_DEFAULT:

PARAMETER_NAME: DOTNET_ASSEMBLY_FACTORY_NAME_ATTR
PARAMETER_TYPE: OPTIONAL STRING
PARAMETER_PROMPT: .NET factory class:
PARAMETER_DEFAULT:

PARAMETER_NAME: DOTNET_ASSEMBLY_FACTORY_PARAMS_ATTR
PARAMETER_TYPE: OPTIONAL STRING
PARAMETER_PROMPT: User Parameters:
PARAMETER_DEFAULT:

TOOLTIP_START
.NET Class to Process Features: 
   $(DOTNET_ASSEMBLY_FILENAME_ATTR) -> $(DOTNET_ASSEMBLY_FACTORY_NAME_ATTR)
TOOLTIP_END

# =========================================================================

TEMPLATE_START

FACTORY_DEF * FMEDotNetWrapperFactory
   FACTORY_NAME $(XFORMER_NAME)
   $(INPUT_LINES)
   DOTNET_ASSEMBLY_FILENAME DotNetCaller DOTNET_ASSEMBLY_FILENAME "$(DOTNET_ASSEMBLY_FILENAME_ATTR)"
   DOTNET_ASSEMBLY_FACTORY_NAME "$(DOTNET_ASSEMBLY_FACTORY_NAME_ATTR)"
   DOTNET_ASSEMBLY_FACTORY_PARAMS "$(DOTNET_ASSEMBLY_FACTORY_PARAMS_ATTR)"
   OUTPUT OUTPUT FEATURE_TYPE $(OUTPUT_OUTPUT_FTYPE) $(OUTPUT_OUTPUT_FUNCS)

TEMPLATE_END

# =========================================================================
