   SOURCE_FILENAME_LIST_ATTR "$(SOURCE_FILENAME_LIST)"
   SOURCE_FIELD_LIST_ATTR "$(SOURCE_FIELD_LIST)"
   TARGET_FILENAME_ATTR "$(TARGET_FILENAME)"
   OUTPUT OUTPUT FEATURE_TYPE $(OUTPUT_OUTPUT_FTYPE) $(OUTPUT_OUTPUT_FUNCS)

TEMPLATE_END

